let rec member = \elem. \list. 
        if (hd list) = elem then 1 else
            if (hd list) = (tl list) then 0 else
                member elem (tl list)
in
member 1 2:3:1:#

;;

let rec remove = \elem. \list. 
        if (hd (tl list)) = elem then (hd list) : (tl (tl list)) else
            if (hd list) = (tl list) then 0 else
                remove elem (tl list)
in
remove 2 2:3:1:#

;;


