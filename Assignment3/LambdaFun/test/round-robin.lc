val newCList = \e.
    let val a = new [] in
    a := [e,a];
    a
;;

-- complete the following functions

val next = \a. 
    head(tail !a) 
;;


val get = \a. 
    head(!a) 
;;

val update = \e. \a. 
    a := [e, next a] 
;;

val insert = \e. \a.
    let val b = newCList e in
    b := [get b,next(a)];
    a := [get a, b]
;;



val delete = \a. 
    a := [get a,head (tail (!(next a)))] ;;

